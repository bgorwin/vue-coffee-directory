lor=\"teal\"\n              dark\n              :to=\"city.url\"\n            >{{ city.label }}</v-btn>\n          </div>\n        </div>\n      </v-col>\n    </v-row>\n\n    <v-row class=\"d-flex flex-direction-row\">\n      <v-col class=\"d-flex flex-row flex-wrap justify-center order-2\" cols=\"12\">\n        <div class=\"mr-4 mb-6 coffeeShopCards\" v-for=\"shop in shops\" :key=\"`${shop.id}`\">\n          <CoffeeCards :shop=\"shop\" />\n        </div>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n// :to=\"city.url\"\n\n<script>\nimport CoffeeCards from \"../components/CoffeeCards\";\nimport shopData from \"../data/shops.json\";\n\nexport default {\n  components: {\n    CoffeeCards\n  },\n  data() {\n    return {\n      shops: shopData,\n      cities: [\n        {\n          label: \"Austin\",\n          url: \"/austin\"\n        },\n        {\n          label: \"Dallas\",\n          url: \"/dallas\"\n        },\n        {\n          label: \"San Francisco\",\n          url: \"/sanfrancisco\"\n        },\n        {\n          label: \"Los Angeles\",\n          url: \"/losangeles\"\n        }\n      ]\n    };\n  },\n  methods: {}\n};\n</script>\n\n<style scoped>\n.border {\n  border: 1px solid red;\n}\n\n.austin-heading {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  color: white;\n  font-size: 60px;\n}\n</style>\n"]}]}